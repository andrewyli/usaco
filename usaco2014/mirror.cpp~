#include <iostream>
#include <fstream>

int main()
{
    ifstream fin("mirror.in");
    ofstream fout("mirror.out");
    
    int n, m;
    fin >> n >> m;
    
    char field[n][m];
    char buffer;
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            fin >> buffer;
            field[i][j] = buffer;
            cout << field[i][j]
        }
        cout << "\n"
    }
}
